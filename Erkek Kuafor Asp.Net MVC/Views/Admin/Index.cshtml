@using Deneme09.Models.Siniflar
@model List<Hizmetler>



@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}





<table class="table table-bordered mt-3">
    <tr>
        <th> Id</th>
        <th> İşlem</th>
        <th> İşlem Açıklaması</th>
        <th> Fiyat</th>
        <th> Güncelle</th>
    </tr>
    @foreach (var x in Model)
    {
<tr>
    <td>@x.id</td>
    <td>@x.islem</td>
    <td>@x.islemacıkla</td>
    <td>@x.fiyat</td>
    <td><a href="/Admin/HizmetlerGetir/@x.id" class="btn btn-primary"> Güncelle</a></td>
</tr>
    }


</table>

